{
  "name": "ractive-router",
  "version": "0.0.1",
  "description": "A Simple Router for Ractive.js",
  "author": {
    "name": "Fay√ßal Tirich"
  },
  "main": "dist/ractive-router.umd.js",
  "scripts": {
    "build": "rm -rf dist && npm run build:umd && npm run build:es",
    "build:umd": "rollup -c -f umd -o dist/ractive-router.umd.js",
    "build:es": "rollup -c -f es6 -o dist/ractive-router.es.js",
    "prepublish": "npm run build",
    "test": "mocha --compilers js:babel-core/register --colors -w ./test/*.js",
    "demo": "webpack-dev-server --inline --hot --config demo/webpack.config.js --content-base demo --history-api-fallback --host 0.0.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://fayway@bitbucket.org/fayway/ractive-router.git"
  },
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-core": "^6.6.5",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "chai": "^3.5.0",
    "mocha": "^2.4.5",
    "ractive": "^0.7.3",
    "webpack": "^1.12.14",
    "webpack-dev-server": "^1.14.1",
    "babel-preset-es2015-rollup": "^1.1.1",
    "rollup": "^0.25.4",
    "rollup-plugin-babel": "^2.4.0"
  },
  "dependencies": {
    "crossroads": "^0.12.2",
    "hasher": "^1.2.0"
  },
  "license": "MIT"
}
